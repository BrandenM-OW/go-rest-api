{
    "swagger": "2.0",
    "info": {
        "description": "This is a server for uploading and downloading files.",
        "title": "Files API",
        "contact": {
            "name": "Branden Morin",
            "email": "brandenmorin14@gmail.com"
        },
        "version": "1.0"
    },
    "host": "localhost",
    "basePath": "/",
    "paths": {
        "/Files": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Retrieves all Files",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.File"
                            }
                        }
                    }
                }
            }
        },
        "/files": {
            "post": {
                "produces": [
                    "application/json"
                ],
                "summary": "Creates an File",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Title",
                        "name": "title",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Content",
                        "name": "content",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.File"
                        }
                    }
                }
            }
        },
        "/files/{id}": {
            "delete": {
                "produces": [
                    "application/json"
                ],
                "summary": "Deletes a File based on given ID",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "File ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.File"
                        }
                    }
                }
            }
        },
        "/files/{name}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Retrieves a File based on given Name",
                "parameters": [
                    {
                        "type": "string",
                        "description": "File Name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.File"
                        }
                    }
                }
            }
        },
        "/getpresigned": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Gets a presigned URL for uploading a file to GCS",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "definitions": {
        "models.File": {
            "type": "object",
            "properties": {
                "cachedUrl": {
                    "type": "string"
                },
                "createdAt": {
                    "type": "string"
                },
                "deletedAt": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        }
    }
}